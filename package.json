{
  "name": "dotparser-normalized",
  "version": "0.1.0",
  "description": "to parse GraphViz",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
